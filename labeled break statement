package test;
import java.util.*;

        
public class demo {
	public static void main(String[] args )
	{
		
		//
		shah:
		for(int i=1;i<=5;i++)
		{
			for(int j=1;j<=4;j++)
			{
				if(i==4)
					break shah;     //labeled break statement
				System.out.print(" * ");
			}
			System.out.println(" ");
		}
	}
	
}

	
