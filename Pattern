package test;

/*
 1
 01
 101
 0101
 10101
 010101
 1010101
 01010101 
 */
 
public class demo {
	public static void main(String[] args )
	{
		for(int i=1;i<=8;i++)
		{
			for(int j=1;j<=8;j++)
			{
				if((i+j)%2==0)
				{
					System.out.print("1");
				}
				else
					System.out.print("0");
			}
			System.out.println("");
		}
	}
}

	
