package test;
import java.util.*;
						  
public class demo extends Thread {
	 public void run(){  
		   System.out.println("running...");  
		 }     
		
	public static void main(String[] args)   
	{
	demo t1 = new demo();
	demo t2 = new demo();
	t1.start();  
	t2.start();  
	System.out.println("Name of the first  thread is  :"+t1.getName());
	System.out.println("Name of the first  thread is  :"+t2.getName());
	t1.setName("my first thread");
	t2.setName("my Second thread");
	System.out.println("Thread After changing the name  : ");
	System.out.println("Name of the first  thread is  :"+t1.getName());
	System.out.println("Name of the first  thread is  :"+t2.getName());
	
		
}
}
  






		


