package test;
import java.util.*;


public class demo {
	
	public static void main(String[] args)   
	{
		 Scanner sc = new Scanner(System.in);
		 System.out.println("How many rows you want to enter : ");
		 int rows = sc.nextInt();
		 int alphabate=65;
		 
		 for(int i=0;i<=rows;i++)
			 
		 {
			 
			 for(int j=0;j<=i;j++)
			 {
				 
				 System.out.print((char)(alphabate+j)+" ");
				 
			 }
			 
			 System.out.println(" ");
			 
		 }
		
		 
}
}







		


