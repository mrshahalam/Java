package  first_project;									
import java.util.*;
import java.util.regex.*;
import java.io.FileNotFoundException;
import java.io.PrintWriter;  
					 
					   
		 public class first_class extends Thread
		 {
			 public void  run()
			 {
				  System.out.println("the thread name is "+Thread.currentThread().getName()); 
				  System.out.println("the  priority of the thread id :"+Thread.currentThread().getPriority());
			 }
			
			 
		public  static void main (String[] args)
		
		{
			first_class obj1 = new first_class();
			first_class obj2 = new first_class();
			first_class obj3 = new first_class();
			first_class obj4 = new first_class();

			obj1.setPriority(MIN_PRIORITY);
			obj2.setPriority(NORM_PRIORITY);
			obj3.setPriority(MAX_PRIORITY);
			//obj4.setPriority(9);
			obj1.start();
			obj2.start();
			obj3.start();
			//obj4.start();

			


			

		}
		 }
					 
		 
	 
		 
		 

		

		
	  
	  
		
		    
				
		
				
								
