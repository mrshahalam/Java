package test;
import java.util.Arrays;
import java.util.Scanner;  
/*
 * Lower triangular matrix:
1	 0   0
8    6   0
4    5   6
*/
public  class demo {
	
	public static void main(String[] args)   throws Exception
	{ 
		
		int m[][]={
				{4,1,1},
				{7,1,2},
				{3,2,2}
				};
		   
		for(int i=0;i<m[0].length;i++)
		{
			
			for(int j=0;j<m.length;j++)
			{
				System.out.print(""+m[j][i]);
				
			}
			System.out.println();
					
	    }
		//---------------------------------
		
}
}




