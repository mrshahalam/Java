package  first_project;
										
import java.util.*; 	

							 class Student 
							{ 
								 
								
								
							    } 
			public  class first_class {
				
				public  static void main (String[] args)
				{ 
					Scanner yy  = new Scanner(System.in);
					System.out.print("Enter the no\n");

					int x= yy.nextInt();
					int s=0,r;
					while(x>0)
					{
							r=x%10;
							x=x/10;
							s=s*10+r;
					}
					System.out.print("The Reverse of the  number is :"+s);
				
				}
			}
					
