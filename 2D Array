
package test;
import java.util.*;
/*
Prime number or note

 */

public class demo {
	public static void main(String[] args )
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("How many row you want  to enter : ");
		int r=sc.nextInt();
		System.out.println("How many column you want  to enter : ");
		int c=sc.nextInt();
		 int p[][]= new int[r][c];
		 System.out.println("Enter the number in the matrixs");
		 for(int i=0;i<=r;i++)
		 {
			 for(int j=0;j<=c;j++)
			 {
				 p[i][j]=sc.nextInt();
			 }
		 }
		 System.out.print("which  row position element you want to see : ");
		 int r1=sc.nextInt();
		 System.out.print("which  column position element you want to see : ");
		 int c1=sc.nextInt();
		 System.out.print(p[r1][c1]+" ");
    } 
}

	
