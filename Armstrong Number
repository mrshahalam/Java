package test;
import java.util.Scanner;  

/*
 *   Armstrong of the number 
*/

public  class demo {
	
	public static void main(String[] args)   throws Exception
	{ 
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter the number :  ");
		int n = sc.nextInt();
		int p=n;
		int sum=0,r;
		while(n>0)
		{
		  r=n%10;
		  sum=sum+r*r*r;
		  n=n/10;
		}
		if(sum==p)
			System.out.printf("Given  number is Armstrong.. ");
		else
			System.out.printf("Given   number not is Armstrong.. ");

		
	}
}
